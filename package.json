{
  "name": "agent-skill-harbor",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Internal agent skill governance and catalog for organizations",
  "license": "MIT",
  "packageManager": "pnpm@10.30.2",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "collect": "tsx --env-file-if-exists=.env scripts/collect-org-skills.ts",
    "build:catalog": "tsx --env-file-if-exists=.env scripts/build-catalog.ts",
    "dev": "pnpm --filter agent-skill-harbor-web dev",
    "build": "pnpm run build:catalog && pnpm --filter agent-skill-harbor-web build",
    "preview": "pnpm --filter agent-skill-harbor-web preview",
    "lint": "pnpm --filter agent-skill-harbor-web lint",
    "check": "pnpm --filter agent-skill-harbor-web check"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["esbuild"]
  },
  "devDependencies": {
    "@octokit/rest": "^21.1.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.15.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  }
}
